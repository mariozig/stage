Gallery Index:
<div>
  <% @galleries.each do |gallery| %>
    <div>
      <%= link_to gallery.title, gallery_path(gallery) %> in category: <%= link_to gallery.category.name, category_path(gallery.category) %>
    </div>
  <% end %>
</div>

<!-- Pagination -->
<%= render 'galleries/paginated_listing', pagy: @pagy %>
<!-- End Pagination -->
